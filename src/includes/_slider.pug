    <!-- Swiper -->
-var arrImg = ["template-1", "template-2", "template-3", "template-4", "template-5", "template-6", "template-7", "template-8", "template-9", "template-10", "template-11", "template-12", "template-13", "template-14", "template-15", "template-16", "template-17"];

div(class="swiper mySwiper")
    div(class="swiper-wrapper")
        each val in arrImg
            div(class="swiper-slide")
                a(href="gallery/" + val + ".html" target="_blank")
                    img(src="assets/img/slider/small-" + val + ".png", alt=val)
    div(class="swiper-button-next")
    div(class="swiper-button-prev")
    div(class="swiper-pagination")



